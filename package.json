{
  "name": "ave-react-template",
  "version": "0.0.1",
  "description": "quick start for ave-react",
  "main": "src/app.js",
  "scripts": {
    "dev": "tsnd --respawn ./src/app.tsx",
    "dev:once": "ts-node ./src/app.tsx",
    "prebuild": "del-cli ./build",
    "build": "tsc",
    "postbuild": "copyfiles ./assets/* ./build",
    "prerelease": "npm run build",
    "release": "ave-pack pack"
  },
  "author": "ivjtk; rerender2021",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^17.0.21",
    "@types/react": "^17.0.0",
    "ave-pack": "^0.9.4",
    "copyfiles": "^2.4.1",
    "del-cli": "^4.0.1",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "ave-react": "^0.1.2"
  }
}
